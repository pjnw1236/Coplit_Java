package com.codestates.coplit; 
import java.util.*;

public class Solution { 
	public boolean or(boolean[] arr){
  
    // arr가 빈 배열이면 false 반환
    if (arr.length == 0) { return false; }
    
    boolean head = arr[0]; 
    boolean [] tail = Arrays.copyOfRange(arr, 1, arr.length); 

    return head || or(tail);
    
	} 
}
